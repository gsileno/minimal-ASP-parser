   "source": [
    "from ASPLoader import *\n",
    "\n",
    "program = parse_string(\"b :- not a. -c :- a. a.\")\n",
    "answer_sets = program.solve()\n",
    "\n",
    "print(\"Answer sets\")\n",
    "\n",
    "for i in range(len(answer_sets)):\n",
    "    atoms = []\n",
    "    for atom in answer_sets[i]:\n",
    "        atoms.append(str(atom))\n",
    "    print(\"%d: %s\" % (i+1, str(atoms)))"
